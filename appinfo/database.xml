<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>
	<charset>utf8</charset>
	<table>
		<name>*dbprefix*etherpad_pads</name>
		<declaration>
			<field>
				<name>id</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<autoincrement>1</autoincrement>
				<length>4</length>
			</field>
			<field>
				<name>uid</name>
				<type>text</type>
				<default></default>
				<notnull>true</notnull>
				<length>64</length>
			</field>
			<field>
				<name>path</name>
				<type>text</type>
				<default></default>
				<notnull>true</notnull>
				<length>512</length>
			</field>
			<field>
				<name>padid</name>
				<type>text</type>
				<default></default>
				<notnull>true</notnull>
				<length>50</length>
			</field>
			<index>
				<name>id_pKey</name>
				<primary>true</primary>
				<field>
					<name>id</name>
					<sorting>ascending</sorting>
				</field>
			</index>
			<index>
				<name>padid_unique</name>
				<unique>true</unique>
				<field>
					<name>padid</name>
					<sorting>ascending</sorting>
				</field>
			</index>
			<index>
				<name>uid_index</name>
				<field>
					<name>uid</name>
					<sorting>ascending</sorting>
				</field>
			</index>
		</declaration>
	</table>
